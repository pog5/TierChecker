{
	"schemaVersion": 1,
	"id": "tierchecker",
	"version": "${version}",
	"name": "TierChecker",
	"description": "Check other players tiers using a command via the MCTiers APIs.",
	"authors": [
		"pog5"
	],
	"contributors": [
		"skeletica."
	],
	"contact": {
		"sources": "https://github.com/pog5/TierChecker",
		"homepage": "https://github.com/pog5/TierChecker",
		"issues": "https://github.com/pog5/TierChecker/issues"
	},
	"license": "MIT",
	"icon": "assets/tierchecker/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "me.pog5.tierchecker.TierChecker",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "me.pog5.tierchecker.TierCheckerClient",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			{
				"value": "me.pog5.tierchecker.modmenu.ModMenuIntegration",
				"adapter": "kotlin"
			}
		]
	},
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"yet_another_config_lib_v3": ">=3.6.6+1.21.4-fabric"
	},
	"suggests": {
		"modmenu": "*"
	}
}